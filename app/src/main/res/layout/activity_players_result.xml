<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ballinapp.com.ballinapp.search.PlayersResult"
    android:background="@drawable/update_team_wp">

    <ListView
        android:layout_below="@+id/players_tv_src"
        android:layout_marginTop="@dimen/_5sdp"
        android:id="@+id/players_list_view_src"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_150sdp"
        android:layout_alignParentBottom="true">

    </ListView>

    <ImageView
        android:id="@+id/players_tv_src"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@drawable/players_tv_list"
        android:contentDescription="@string/players"/>

</RelativeLayout>